#include <zephyr/dt-bindings/gpio/gpio.h>

&zephyr_udc0 {
	cdc_acm_uart0 {
		compatible = "zephyr,cdc-acm-uart";
		label = "CDC_ACM_0";
		parity = "none";
		current-speed = <115200>;
	};
};

/ { 
	zephyr,user {
    	poweron-gpios = <&gpio0 2 GPIO_ACTIVE_HIGH>;
	};
};
